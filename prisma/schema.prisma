// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model ProgressLog {
  id String @id @default(uuid())
  userID String
  createdAt DateTime @default(now())
  type Int
  product Int
  summary String
}