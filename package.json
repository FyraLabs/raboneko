{
  "name": "raboneko",
  "version": "0.25.0",
  "description": "Raboneko.",
  "main": "dist/index.js",
  "engines": {
    "node": ">=19.7.0"
  },
  "scripts": {
    "sync": "slash-up sync",
    "sync:dev": "slash-up sync -e development",
    "start": "cd dist && node index.js",
    "dev": "tsc-watch --noClear --onSuccess \"node ./dist/index.js\"",
    "build": "npx tsc",
    "check": "tsc --noEmit",
    "prettier:check": "prettier ./src --check",
    "eslint:check": "eslint ./src/**/*.ts",
    "prettier:fix": "prettier ./src --write",
    "eslint:fix": "eslint ./src/**/*.ts --fix",
    "lint": "pnpm run prettier:check && pnpm run eslint:check && pnpm run check",
    "lint:fix": "pnpm run prettier:fix && pnpm run eslint:fix",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org fyralabs --project raboneko ./dist && sentry-cli sourcemaps upload --org fyralabs --project raboneko ./dist"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "@sentry/node": "^10.24.0",
    "bullmq": "^5.66.0",
    "cat-loggr": "^1.2.2",
    "dayjs": "^1.11.19",
    "discord-api-types": "^0.38.37",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.2",
    "parse-duration": "^2.1.4",
    "slash-create": "^6.7.0"
  },
  "devDependencies": {
    "@sentry/cli": "^2.58.2",
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^9.39.1",
    "eslint-config-dmitmel": "github:dmitmel/eslint-config-dmitmel",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "prisma": "^7.1.0",
    "slash-up": "^1.4.2",
    "ts-node": "^10.9.2",
    "tsc-watch": "^7.2.0",
    "typescript": "^5.9.3"
  }
}
