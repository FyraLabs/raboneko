{
  "name": "raboneko",
  "version": "0.25.0",
  "description": "Raboneko.",
  "main": "src/index.js",
  "engines": {
    "bun": ">=1.3.0"
  },
  "scripts": {
    "sync": "slash-up sync",
    "sync:dev": "slash-up sync -e development",
    "start": "cd src && bun run ./index.js",
    "dev": "bun --watch --no-clear-screen run ./src/index.js",
    "check": "tsc --noEmit",
    "prettier:check": "prettier ./src --check",
    "eslint:check": "eslint ./src/**/*.ts",
    "prettier:fix": "prettier ./src --write",
    "eslint:fix": "eslint ./src/**/*.ts --fix",
    "lint": "bun run prettier:check && bun run eslint:check && bun run check",
    "lint:fix": "bun run prettier:fix && bun run eslint:fix"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@sentry/node": "^10.38.0",
    "bullmq": "^5.67.3",
    "cat-loggr": "^1.2.2",
    "dayjs": "^1.11.19",
    "discord-api-types": "^0.38.38",
    "discord.js": "^14.25.1",
    "fastify": "^5.7.4",
    "parse-duration": "^2.1.5",
    "prisma": "^7.3.0",
    "slash-create": "^6.7.1"
  },
  "devDependencies": {
    "@sentry/cli": "^3.1.0",
    "@types/bun": "^1.3.8",
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.1",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^10.0.0",
    "eslint-config-dmitmel": "github:dmitmel/eslint-config-dmitmel",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.5",
    "prettier": "^3.8.1",
    "slash-up": "^1.4.2",
    "typescript": "^5.9.3"
  }
}
