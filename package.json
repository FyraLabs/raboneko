{
  "name": "raboneko",
  "version": "0.25.0",
  "description": "Raboneko.",
  "main": "dist/index.js",
  "engines": {
    "node": ">=19.7.0"
  },
  "scripts": {
    "sync": "slash-up sync",
    "sync:dev": "slash-up sync -e development",
    "start": "cd dist && node index.js",
    "dev": "tsc-watch --noClear --onSuccess \"node ./dist/index.js\"",
    "build": "bunx tsc",
    "check": "tsc --noEmit",
    "prettier:check": "prettier ./src --check",
    "eslint:check": "eslint ./src/**/*.ts",
    "prettier:fix": "prettier ./src --write",
    "eslint:fix": "eslint ./src/**/*.ts --fix",
    "lint": "bun run prettier:check && bun run eslint:check && bun run check",
    "lint:fix": "bun run prettier:fix && bun run eslint:fix",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org fyralabs --project raboneko ./dist && sentry-cli sourcemaps upload --org fyralabs --project raboneko ./dist"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@sentry/node": "^10.36.0",
    "bullmq": "^5.67.2",
    "cat-loggr": "^1.2.2",
    "dayjs": "^1.11.19",
    "discord-api-types": "^0.38.37",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "fastify": "^5.7.2",
    "parse-duration": "^2.1.5",
    "slash-create": "^6.7.1"
  },
  "devDependencies": {
    "@sentry/cli": "^3.1.0",
    "@types/bun": "^1.3.6",
    "@types/express": "^5.0.6",
    "@types/node": "^25.1.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.2",
    "eslint-config-dmitmel": "github:dmitmel/eslint-config-dmitmel",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.5",
    "prettier": "^3.8.1",
    "prisma": "^7.3.0",
    "slash-up": "^1.4.2",
    "ts-node": "^10.9.2",
    "tsc-watch": "^7.2.0",
    "typescript": "^5.9.3"
  }
}
