{
  "name": "raboneko",
  "version": "0.22.0",
  "description": "Raboneko.",
  "main": "dist/index.js",
  "engines": {
    "node": ">=19.7.0"
  },
  "scripts": {
    "sync": "slash-up sync",
    "sync:dev": "slash-up sync -e development",
    "start": "cd dist && node index.js",
    "dev": "tsc-watch --noClear --onSuccess \"node ./dist/index.js\"",
    "build": "npx tsc",
    "check": "tsc --noEmit",
    "prettier:check": "prettier ./src --check",
    "eslint:check": "eslint ./src/**/*.ts",
    "prettier:fix": "prettier ./src --write",
    "eslint:fix": "eslint ./src/**/*.ts --fix",
    "lint": "pnpm run prettier:check && pnpm run eslint:check && pnpm run check",
    "lint:fix": "pnpm run prettier:fix && pnpm run eslint:fix"
  },
  "dependencies": {
    "@prisma/client": "^6.16.1",
    "bullmq": "^5.58.5",
    "cat-loggr": "^1.2.2",
    "dayjs": "^1.11.18",
    "discord-api-types": "^0.38.26",
    "discord.js": "^14.21.0",
    "dotenv": "^17.2.2",
    "fastify": "^5.6.0",
    "parse-duration": "^2.1.4",
    "slash-create": "^6.6.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.35.0",
    "eslint-config-dmitmel": "github:dmitmel/eslint-config-dmitmel",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "prisma": "^6.16.2",
    "slash-up": "^1.4.2",
    "ts-node": "^10.9.2",
    "tsc-watch": "^7.0.0",
    "typescript": "^5.9.2"
  }
}
